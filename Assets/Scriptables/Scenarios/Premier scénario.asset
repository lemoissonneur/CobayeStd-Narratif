  m_Script: {fileID: 11500000, guid: 0c0d158e52ff26d4baa1f02b020abef6, type: 3}
  m_Name: pas de cauchemar
  m_EditorClassIdentifier: 
  KeyString: "Cette nuit, L\xE9onard va avoir de la chance"
  Description: "C\u2019est l\u2019exception qui fait la r\xE8gle dit-on. L\xE9onard
    trouvait ce proverbe stupide. Mais c\u2019est la premi\xE8re chose qui lui vint
    \xE0 l\u2019esprit le lendemain apr\xE8s avoir dormi toute la nuit comme un loir.
    Pas le moindre cauchemar \xE0 l\u2019horizon. De nos jours, mal dormir \xE9tait
    la r\xE8gle. La nuit qui venait de se terminer \xE9tait une exception. Et le probl\xE8me
    avec les exceptions\u2026"
  Format: {fileID: 11400000, guid: 593c449e67de7434da72718480b03099, type: 2}
  NextSituation: {fileID: 114125655037699612}
--- !u!114 &114353638917813634
MonoBehaviour:
  m_ObjectHideFlags: 0
