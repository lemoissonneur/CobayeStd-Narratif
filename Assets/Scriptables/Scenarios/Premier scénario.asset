  m_Script: {fileID: 11500000, guid: 0c0d158e52ff26d4baa1f02b020abef6, type: 3}
  m_Name: "Le caf\xE9 c\u2019est la vie"
  m_EditorClassIdentifier: 
  KeyString: "Le caf\xE9 c\u2019est la vie"
  Description: "Le caf\xE9 est cher, mais les affaires allaient assez bien. Son m\xE9tier
    de d\xE9tective n\u2019avait pas uniquement aiguis\xE9 sa curiosit\xE9, mais aussi
    son flair. Et en ce moment m\xEAme, c\u2019\xE9tait lui qui parlait \xE0 l\u2019esprit
    de L\xE9onard. \r\nSon flair lui disait : l\xE2che pas ce client, il y a une grosse
    somme \xE0 la cl\xE9. Prendre un caf\xE9 maintenant, c\u2019\xE9tait l\u2019assurance
    de para\xEEtre pr\xE9sentable et attentif lors de l\u2019entretien. Ce n\u2019\xE9tait
    pas du gaspillage. C\u2019\xE9tait un investissement sur l\u2019avenir."
  Format: {fileID: 11400000, guid: 593c449e67de7434da72718480b03099, type: 2}
  NextSituation: {fileID: 0}
--- !u!114 &114358143619864276
MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
